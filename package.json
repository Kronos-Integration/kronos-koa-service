{
  "name": "@kronos-integration/service-http",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/service-http.mjs",
  "exports": {
    ".": "./src/service-http.mjs"
  },
  "description": "http server",
  "keywords": [
    "http",
    "jwt",
    "kronos-service",
    "websocket"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/service-http.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/service-http.mjs"
  },
  "dependencies": {
    "@kronos-integration/endpoint": "^9.2.17",
    "@kronos-integration/interceptor": "^10.0.11",
    "@kronos-integration/service": "^9.2.27",
    "bufferutil": "^4.0.2",
    "jsonwebtoken": "^8.5.1",
    "model-attributes": "^4.1.7",
    "multi-path-matcher": "^2.1.3",
    "utf-8-validate": "^5.0.3",
    "ws": "^7.4.0"
  },
  "devDependencies": {
    "@kronos-integration/test-interceptor": "^7.0.7",
    "ava": "^3.13.0",
    "c8": "^7.3.5",
    "documentation": "github:arlac77/documentation",
    "got": "^11.8.0",
    "network-address": "^1.1.2",
    "semantic-release": "^17.3.0"
  },
  "engines": {
    "node": ">=14.15.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/service-http.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/service-http/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/service-http#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-github-action",
      "arlac77/template-kronos-component"
    ]
  }
}
