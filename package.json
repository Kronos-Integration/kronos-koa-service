{
  "name": "kronos-service-koa",
  "version": "0.0.0-semantic-release",
  "description": "koa backed http server",
  "keywords": [
    "kronos-service"
  ],
  "main": "dist/module.js",
  "module": "src/ServiceKOA.js",
  "scripts": {
    "cover": "nyc npm test",
    "test": "mocha tests/*-test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "pretest": "rollup -c",
    "posttest": "markdown-doctest",
    "precover": "rollup -c",
    "prepare": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kronos-Integration/kronos-service-koa.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-service-koa/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-service-koa#readme",
  "dependencies": {
    "koa-jwt": "3.2.2",
    "koa-static": "4.0.1",
    "kronos-koa": "^1.2.2",
    "kronos-service": "^4.8.2",
    "model-attributes": "^3.0.10",
    "path-to-regexp": "^2.0.0",
    "ws": "^3.1.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "koa-route": "^3.2.0",
    "mocha": "^3.5.3",
    "network-address": "^1.1.2",
    "semantic-release": "^8.2.3",
    "supertest": "^3.0.0",
    "supertest-as-promised": "^4.0.2",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.51.0",
    "nyc": "^11.3.0"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=8.8.1"
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/Kronos-Tools/npm-package-template.git"
    }
  },
  "xo": {
    "space": true
  }
}
