{
  "name": "kronos-service-koa",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/service-koa.js",
  "module": "src/service-koa.js",
  "description": "koa backed http server",
  "keywords": [
    "kronos-service"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "docs": "documentation readme src/service-koa.js --section=API",
    "lint": "documentation lint src/service-koa.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "test": "ava"
  },
  "dependencies": {
    "koa-jwt": "3.5.1",
    "koa-static": "5.0.0",
    "kronos-koa": "^2.0.0",
    "kronos-service": "^4.10.0",
    "model-attributes": "^3.0.10",
    "path-to-regexp": "^2.2.1",
    "ws": "^6.0.0"
  },
  "devDependencies": {
    "ava": "^1.1.0",
    "c8": "^3.2.1",
    "documentation": "^9.1.1",
    "esm": "^3.1.0",
    "got": "^9.4.0",
    "koa-route": "^3.2.0",
    "markdown-doctest": "^0.9.1",
    "network-address": "^1.1.2",
    "rollup": "^1.1.0",
    "rollup-plugin-cleanup": "^3.1.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-executable": "^1.4.3",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "semantic-release": "^15.13.3"
  },
  "engines": {
    "node": ">=10.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-service-koa.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-service-koa/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-service-koa#readme",
  "ava": {
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "js",
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
