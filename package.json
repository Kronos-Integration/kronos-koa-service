{
  "name": "kronos-service-koa",
  "version": "0.0.0-semantic-release",
  "description": "koa backed http server",
  "keywords": [
    "kronos-service"
  ],
  "main": "dist/module.js",
  "module": "src/ServiceKOA.js",
  "scripts": {
    "cover": "nyc npm test",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "prepublish": "rollup --output=dist/module.js -c -- src/ServiceKOA.js",
    "pretest": "npm run prepublish",
    "posttest": "markdown-doctest",
    "precover": "npm run prepublish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-service-koa.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-service-koa/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-service-koa#readme",
  "dependencies": {
    "koa-jwt": "^3.2.1",
    "koa-static": "^3.0.0",
    "kronos-koa": "^1.2.1",
    "kronos-service": "^4.5.5",
    "model-attributes": "^3.0.8",
    "path-to-regexp": "^1.7.0",
    "ws": "^2.3.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^2.0.0",
    "koa-route": "^3.2.0",
    "mocha": "^3.2.0",
    "network-address": "^1.1.2",
    "semantic-release": "^6.3.6",
    "supertest": "^3.0.0",
    "supertest-as-promised": "^4.0.2",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.41.6",
    "nyc": "^10.2.0"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=7.9.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/Kronos-Tools/npm-package-template.git"
    }
  }
}