{
  "name": "kronos-service-koa",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/service-koa.js",
  "module": "src/service-koa.js",
  "description": "koa backed http server",
  "keywords": [
    "kronos-service"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "test": "ava",
    "semantic-release": "semantic-release",
    "docs": "documentation readme src/service-koa.js --section=API",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "precover": "rollup -c tests/rollup.config.js",
    "prepare": "rollup -c",
    "lint": "documentation lint src/service-koa.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "koa-jwt": "3.5.0",
    "koa-static": "5.0.0",
    "kronos-koa": "^2.0.0",
    "kronos-service": "^4.10.0",
    "model-attributes": "^3.0.10",
    "path-to-regexp": "^2.2.1",
    "ws": "^6.0.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "documentation": "^8.1.2",
    "got": "^9.0.0",
    "koa-route": "^3.2.0",
    "markdown-doctest": "^0.9.1",
    "network-address": "^1.1.2",
    "nyc": "^13.0.1",
    "rollup": "^0.65.2",
    "rollup-plugin-multi-entry": "^2.0.2",
    "semantic-release": "^15.9.12",
    "travis-deploy-once": "^5.0.7",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-executable": "^1.3.0",
    "rollup-plugin-cleanup": "^3.0.0"
  },
  "engines": {
    "node": ">=10.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-service-koa.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-service-koa/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-service-koa#readme",
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [
      "env"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
